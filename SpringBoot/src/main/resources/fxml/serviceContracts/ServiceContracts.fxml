<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.serwis.controller.serviceContracts.ServiceContractsController">
   <children>
      <Button layoutX="74.0" layoutY="113.0" mnemonicParsing="false" onAction="#newContractAction" text="Dodaj umowe" />
      <Button fx:id="backButton" layoutX="75.0" layoutY="740.0" mnemonicParsing="false" onAction="#backAction" text="Powrót" />
      <TableView fx:id="contractsTable" layoutX="215.0" prefHeight="800.0" prefWidth="786.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="50.0" text="L.p" />
          <TableColumn fx:id="nameColumn" prefWidth="96.0" text="Imie" />
            <TableColumn fx:id="surnameColumn" prefWidth="137.0" text="Nazwisko" />
            <TableColumn fx:id="workingTimeColumn" prefWidth="106.0" text="Czas pracy" />
            <TableColumn fx:id="amountForPartsColumn" prefWidth="105.0" text="Kwota na części" />
            <TableColumn fx:id="remainingWorkingTimeColumn" prefWidth="131.0" text="Pozostały czas pracy" />
            <TableColumn fx:id="remainingAmountForPartsColumn" prefWidth="165.0" text="Pozostała kwota na części" />
            <TableColumn fx:id="editColumn" prefWidth="60.0" text="Edytuj" />
            <TableColumn fx:id="historyColumn" prefWidth="55.0" text="Historia" />
        </columns>
         <contextMenu>
            <ContextMenu onAction="#deleteContract">
              <items>
                <MenuItem mnemonicParsing="false" text="Usuń" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TableView>
      <Label layoutX="76.0" layoutY="14.0" text="Wyszukaj:" />
      <TextField fx:id="searchTextField" layoutX="35.0" layoutY="40.0" prefHeight="31.0" prefWidth="167.0" promptText="wyszukaj" />
   </children>
</AnchorPane>
