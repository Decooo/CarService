            <TableColumn fx:id="detailsColumn" prefWidth="57.0" text="Podglad" />
        </columns>
      </TableView>
      <Button layoutX="98.0" layoutY="64.0" mnemonicParsing="false" onAction="#newRepair" text="Nowe zlecenie" />
      <Button layoutX="98.0" layoutY="132.0" mnemonicParsing="false" onAction="#historyAction" text="Historia zleceń" />
      <Button fx:id="logoutButton" layoutX="119.0" layoutY="727.0" mnemonicParsing="false" onAction="#logoutAction" text="Wyloguj" />
      <Button layoutX="108.0" layoutY="199.0" mnemonicParsing="false" onAction="#openCarsListAction" text="Samochody" />
